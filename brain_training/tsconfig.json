{
   "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "noImplicitAny": false,
      "paths": {
         "@/*": ["./*"]
      }
   },
   "include": [
      "interfaces/*",
      "interfaces/types.d.ts",
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      "**/*.d.ts",
      "classes.ts",
      "graphql/queries.ts"
   ],
   "exclude": ["node_modules"]
}
